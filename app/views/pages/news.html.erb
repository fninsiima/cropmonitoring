<div class="container">
	
	<div class="row">
		<div class="col-xs-4 col-sm-2">
			<h3>Coming soon</h3>
		</div>
		<div class="col-xs-12 col-sm-6">
			<h2>News Page</h2>

			<% @new.each do |new|%>
				<h3><%= new.title %></h3>
				<p class="paper"><%= new.description %></p>
				<p><%= time_ago_in_words(new.created_at) %> &nbsp; ago</p>
			<% end %>
		</div>

	</div>
</div>
